    "apollo-server-core": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "chokidar": "^3.5.3",
    "consola": "^2.15.3",
    "express": "^4.18.2",
    "express-history-api-fallback": "^2.2.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "graphql": "^15.4.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.11.2",
    "launch-editor": "^2.6.0",
    "nanoid": "^4.0.0",
    "nexus": "^1.3.0",
    "object-inspect": "^1.12.3",
